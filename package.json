    "pug": "^2.0.3",
    "pug-cli": "^1.0.0-alpha6",
    "resolve-url-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.2",
    "typescript": "^3.2.2",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@types/codemirror": "0.0.72",
    "codemirror": "^5.43.0"
  }
