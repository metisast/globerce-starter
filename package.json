{
  "name": "gb-static",
  "version": "1.0.0",
  "description": "Globerce Ecom project assets files",
  "main": "index.js",
  "scripts": {
    "dev": "webpack --config ./build/webpack.dev.config.js",
    "build": "webpack --config ./build/webpack.prod.config.js",
    "watch": "webpack --watch --config ./build/webpack.dev.config.js",
    "pug": "pug ./src -P -o ./public/",
    "watch-pug": "pug ./src -P -w -o ./public/",
    "server": "http-server -p 8888 -c -1",
    "api": "node server"
  },
  "keywords": [
    "globerce",
    "ecom"
  ],
  "author": "Kurnossov Yevgeniy",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.2",
